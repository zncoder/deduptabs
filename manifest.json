{
  "name": "deduptabs",
  "version": "1.1.1",
  "manifest_version": 2,
  "description": "Close duplicate tabs in the window",
	"permissions": [
		"tabs"
	],
	"icons": {
		"48": "48.png"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": "32.png",
		"default_title": "Dedup tabs"
	}
}
